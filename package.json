  "scripts": {
    "build:all": "nx run-many --target=build --all",
    "build:api-auth": "nx build api-auth",
    "build:netflix": "nx build netflix",
    "build:api-gateway": "nx build api-gateway",
    "build:csv-processor": "nx build csv-processor",
    "start:all": "nx run-many --target=serve --all",
    "start:api-auth": "nx serve api-auth",
    "start:netflix": "nx serve netflix",
    "start:csv-processor": "nx serve csv-processor",
    "start:api-gateway": "nx serve api-gateway",
