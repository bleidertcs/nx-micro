    "prisma:client:push": "prisma db push --schema libs/prisma-client/prisma/schema.prisma",
    "prisma:client:studio": "prisma studio --schema libs/prisma-client/prisma/schema.prisma",
    "prisma:client:validate": "prisma validate --schema libs/prisma-client/prisma/schema.prisma",
    "prisma:client:format": "prisma format --schema libs/prisma-client/prisma/schema.prisma",
    "prisma:client:reset": "prisma migrate reset --schema libs/prisma-client/prisma/schema.prisma",
    "prisma:netflix:generate": "prisma generate --schema libs/prisma-netflix/prisma/schema.prisma",
    "prisma:netflix:migrate": "prisma migrate dev --schema libs/prisma-netflix/prisma/schema.prisma",
    "prisma:netflix:deploy": "prisma migrate deploy --schema libs/prisma-netflix/prisma/schema.prisma",
