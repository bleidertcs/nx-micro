    "prisma:netflix:validate": "prisma validate --schema libs/prisma-netflix/prisma/schema.prisma",
    "prisma:netflix:format": "prisma format --schema libs/prisma-netflix/prisma/schema.prisma",
    "prisma:netflix:reset": "prisma migrate reset --schema libs/prisma-netflix/prisma/schema.prisma",
    "prisma:generate:all": "npm run prisma:client:generate && npm run prisma:netflix:generate",
    "prisma:format:all": "npm run prisma:client:format && npm run prisma:netflix:format"
  },
  "private": true,
  "prisma": {
    "schema": "libs/prisma-client/prisma/schema.prisma"
