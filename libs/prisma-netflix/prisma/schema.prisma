generator client {
  provider = "prisma-client-js"
  output   = "../../../node_modules/@prisma/client-netflix"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_NETFLIX")
}

model NetflixShow {
  show_id      String    @id
  type         String?
  title        String?
  director     String?
  cast_members String?
  country      String?
  date_added   DateTime? @db.Date
  release_year Int?
  rating       String?
  duration     String?
  listed_in    String?
  description  String?

  @@map("netflix_shows")
}
