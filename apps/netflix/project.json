{
  "name": "netflix",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/netflix/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "webpack-cli build",
        "args": [
          "--node-env=production"
        ],
        "cwd": "apps/netflix"
      },
      "configurations": {
        "development": {
          "args": [
            "--node-env=development"
          ]
        }
      }
    },
    "prune-lockfile": {
      "dependsOn": [
        "build"
      ],
      "cache": true,
      "executor": "@nx/js:prune-lockfile",
      "outputs": [
        "{workspaceRoot}/dist/apps/netflix/package.json",
        "{workspaceRoot}/dist/apps/netflix/pnpm-lock.yaml"
      ],
      "options": {
        "buildTarget": "build"
      }
    },
    "copy-workspace-modules": {
      "dependsOn": [
        "build"
      ],
      "cache": true,
      "outputs": [
        "{workspaceRoot}/dist/apps/netflix/workspace_modules"
      ],
      "executor": "@nx/js:copy-workspace-modules",
      "options": {
        "buildTarget": "build"
      }
    },
    "prune": {
      "dependsOn": [
        "prune-lockfile",
        "copy-workspace-modules"
      ],
      "executor": "nx:noop"
    },
    "serve": {
      "continuous": true,
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": [
        "build"
      ],
      "options": {
        "buildTarget": "netflix:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "netflix:build:development"
        },
        "production": {
          "buildTarget": "netflix:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/netflix/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "prisma-generate": {
      "command": "prisma generate",
      "options": {
        "cwd": "apps/netflix"
      }
    },
    "migrate-dev": {
      "command": "prisma migrate dev",
      "options": {
        "cwd": "apps/netflix"
      }
    },
    "migrate-deploy": {
      "command": "prisma migrate deploy",
      "options": {
        "cwd": "apps/netflix"
      }
    },
    "migrate-reset": {
      "command": "prisma migrate reset",
      "options": {
        "cwd": "apps/netflix"
      }
    },
    "db-pull": {
      "command": "prisma db pull",
      "options": {
        "cwd": "apps/netflix"
      }
    },
    "db-push": {
      "command": "prisma db push",
      "options": {
        "cwd": "apps/netflix"
      }
    },
    "studio": {
      "command": "prisma studio",
      "options": {
        "cwd": "apps/netflix"
      }
    }
  }
}